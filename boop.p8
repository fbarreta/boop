function _init()
	color(17)
	palt(0,true)
	restartgame()
end

function _update()
	moveplayer()
	menuitem(1, menulabel, onecontroller)
end

function _draw()
	cls(7)
	drawgrid()
	drawplayer()
	drawui()
	drawsplash()
	drawgameover()
end
