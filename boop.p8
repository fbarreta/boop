function moveplayer()
	if btnp(0) then
		if checkboundaries("l") then
			player.x -= 1
		end
	elseif btnp(1) then
		if checkboundaries("r") then
			player.x += 1
		end
	elseif btnp(2) then
		if checkboundaries("u") then
			player.y -= 1
		end
	elseif btnp(3) then
		if checkboundaries("d") then
			player.y += 1
		end
	end
	if btnp(4) then
		startgame()
		if canplace() then
			placepiece()
		end
	end
	if btnp(5) then
		startgame()
		changecursor()
	end
end

function changecursor()
	if player1.cursortype == 'k' then
		player1.cursortype = 'c'
	else
		player1.cursortype = 'k'
	end
end

function canplace()
	if player1.kittens > 0 then
		return true
	else
		return false
	end
end

