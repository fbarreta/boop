-- pico boop
-- by fefo
centerx = 5
centery = 0
space = 2
size = 19
spr_k1 = 0
spr_c1 = 2
spr_cursor_k1 = 32
spr_cursor_c1 = 34
spr_cursor_k2 = 36
spr_cursor_c2 = 38
spr_k2 = 4
spr_c2 = 6
p1k = 1
p1c = 2
p2k = 3
p2c = 4
player = {
	x = 0,
	y = 0
}
player1 = {
	kittens = 5,
	cats = 3,
	cursortype = 'k',
	spritek = p1k,
	spritec = p1c,
	cursork = spr_cursor_k1,
	cursorc = spr_cursor_c1,
	kindex = p1k,
	cindex = p1c,
	maxcats = 0
}
player2 = {
	kittens = 8,
	cats = 0,
	cursortype = 'k',
	spritek = p2k,
	spritec = p2c,
	cursork = spr_cursor_k2,
	cursorc = spr_cursor_c2,
	kindex = p2k,
	cindex = p2c,
	maxcats = 0
}
activeplayer = player1
mtx = {}
gamestarted = false
gameover = 0
multicontrollers = false
menulabel = "2 controllers"
