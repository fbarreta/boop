	local p1cats = " cATS:    ".. player1.cats
	
	local p2kittens = " kITTENS: ".. player2.kittens
	local p2cats = " cATS:    ".. player2.cats

	print(p1kittens,2,117,9)
	print(p1cats,2,123,9)
	print(p2kittens,78,117,5)
	print(p2cats,78,123,5)
end

